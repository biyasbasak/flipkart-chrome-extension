{
  "manifest_version":1,
  "name": "price tracker",
  "version": "1.0",
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "*://*/*"
  ]
}
